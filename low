// write a program in c to convert a string to lowercase.

#include<stdio.h>
#include<string.h>
void main()
{
    char a[90];
    printf("Enter the string: ");
   scanf("%[^\n]s",a);
   for(int i=0;a[i]!='\0';i++)
   {
       if(a[i]<97 && a[i]!=' ')
       {
           printf("%c",a[i]+32);
       }
       else
       printf("%c",a[i]);
   }
 
   
}


